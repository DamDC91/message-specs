<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <xsd:attribute name="version">
        <xsd:simpleType>
            <xsd:restriction base="xsd:string">
                <xsd:pattern value="(\d+)\.(\d+)\.(\d+)"/>
            </xsd:restriction>
        </xsd:simpleType>
    </xsd:attribute>
</xsd:schema>
